{
    "name": "GoogleAppScript docker",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "clasp",
    "remoteUser": "root",
    "settings": {
        "terminal.integrated.shell.linux": "zsh"
    },
    "workspaceFolder": "/workdir"
}
